o<place-tool> sub
  o<init-tool-changer-vars> call

  M66 P60 L0
  o7100 if [ #5399 NE 1 ]
    M66  E0 L0
    (abort,No tool in spindle.)
  o7100 endif

  #<toolSlot> = #1

  o7200 if [ #<toolSlot> LT 1 OR #<toolSlot> GT 13 ]
    M66  E0 L0
    (abort,Tool number must be between 1 and 13.)
  o7200 endif

  o[#<toolSlot> + 7000] call

  M61 Q0

o<place-tool> endsub

; tool 1
o7001 sub
  o<local-place-tool> call [#<_tool1X>] [#<_tool1Y>] [#<_tool1Z>]
o7001 endsub

