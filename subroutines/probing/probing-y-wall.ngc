o<probing-y-wall> sub
  #<width> = #1
  #<z> = #2
  #<overTravel> = #3
  #<clearance> = #4
  #<toleranceSizeCheck> = #5
  #<toleranceSize> = #6
  #<tolerancePositionCheck> = #7
  #<tolerancePosition> = #8
  #<updateWCS> = #9
  #<wcs> = #10
  #<printResults> = #11
  #<feed> = #12

  #<probeDiameter> = [ #<_ini[TOOL_PROBE]DIAMETER> * [ #<_metric> * 25.4 + #<_imperial> ] ]
  #<fastFeed> =  [ #<_ini[TOOL_PROBE]FEED_FAST> * [ #<_metric> * 25.4 + #<_imperial> ]] 
  #<backFeed> = [ #<_ini[TOOL_PROBE]FEED_BACK> * [ #<_metric> * 25.4 + #<_imperial> ]] 
  #<slowFeed> = [ #<_ini[TOOL_PROBE]FEED_SLOW> * [ #<_metric> * 25.4 + #<_imperial> ]]

  #<startX> = #5420
  #<startY> = #5421
  #<startZ> = #5422

  #<dY> = [ #<width>*.5 + #<clearance> ]
  #<dZ> = [ #<z> - #<startZ> ]

  o<protected-move-relative> call [ 0 ] [ #<dY> ] [ 0 ] [ #<feed> ]
  o<protected-move-relative> call [ 0 ] [ 0 ] [ #<dZ> ] [ #<feed> ]

  o<probe-relative> call [ 0 ] [ -#<clearance>-#<overTravel> ] [ 0 ] [ #<fastFeed> ] [ #<backFeed> ] [ #<slowFeed> ]

  #<probedY1> = [ #<_probedG5X_Y> - #<probeDiameter> * .5 ]

  o<protected-move-relative> call [ 0 ] [ 0 ] [ -#<dZ> ] [ #<feed> ]
  o<protected-move-relative> call [ 0 ] [ -#<width> - 2 * #<clearance> ] [ 0 ] [ #<feed> ]
  o<protected-move-relative> call [ 0 ] [ 0 ] [ #<dZ> ] [ #<feed> ]

  o<probe-relative> call [ 0 ] [ #<clearance>+#<overTravel> ] [ 0 ] [ #<fastFeed> ] [ #<backFeed> ] [ #<slowFeed> ]

  #<probedY2> = [ #<_probedG5X_Y> + #<probeDiameter> *.5 ]

  #<size> = [ #<probedY1> - #<probedY2> ]
  #<position> = [ [ #<probedY1> + #<probedY2> ] * .5 ]

  #<positionDeviation> = [ #<position> - #<startY> ]
  #<sizeDeviation> = [ #<size> - #<width> ]

  o<protected-move-relative> call [ 0 ] [ 0 ] [ -#<dZ> ] [ #<feed> ]
  o<protected-move-relative> call [ 0 ] [ #<dY> ] [ 0 ] [ #<feed> ]

  o300 if [ #<printResults> EQ 1 ]
    (debug,Probed Y wall y1 = #<probedY1>, y2 = #<probedY2>, Average = #<position>, Size = #<size>)
  o300 endif

  o600 if [ #<tolerancePositionCheck> EQ 1 ]
    o200 if [ ABS [ #<positionDeviation> ]  GT #<tolerancePosition> ]
       M66  E0 L0
      #<absDev> = [ ABS[ #<positionDeviation> ] ]
      (abort,Probing Y wall failed positional tolerance check. Probed #<position>, expected #<startY>. #<absDev> > #<tolerancePosition>)
    o200 endif
  o600 endif

  o700 if [ #<toleranceSizeCheck> EQ 1 ]
    o800 if [ ABS [ #<sizeDeviation> ]  GT #<toleranceSize> ]
       M66  E0 L0
      #<absDev> = [ ABS[ #<sizeDeviation> ] ]
      (abort,Probing Y wall failed size tolerance check. Probed #<size>, expected #<width>. #<absDev> > #<toleranceSize>)
    o800 endif
  o700 endif

  o400 if [ [ #<updateWCS> EQ 1 ] AND [ #<wcs> GE 1 ] AND [ #<wcs> LE 9 ]]
    ; The numbered parameters for the current wcs offsets are always in inches, so convert to active units.
    #<currentWCS_Y> = [ #[5222+20*[#5220-1]] * [ #<_metric> * 25.4 + #<_imperial> ] ]

    ; Then add the probed position
    #<newWCS_Y> = [ #<currentWCS_Y> + #<position> ]

    ; And store it in the appropriate WCS
    G10 L2 P#<wcs> Y#<newWCS_Y>
  o400 endif

o<probing-y-wall> endsub
M2
