;v2-calib-characterize-y
o<v2-calib-characterize-y> sub
  g21
  g90
  #<yMax> = 51.8
  #<yMin> = -63.5
  #<i> = 0
  #<numPoints> = #1
  g0 z0
  g0 x0
  g0 y64.5a0b0

  o100 while [#<i> LT #<numPoints>]
    #<t> = [ #<i> / [ #<numPoints> - 1 ] ]
    #<y> = [[ #<yMax> * (1-#<t>) ] + [ #<yMin> * #<t> ]]
    g0 y[#<y>]
    M66  E0 L0
    o<v2_calib_probe_y> call [#<y>]
    #<i> = [#<i> + 1]
  o100 endwhile
o<v2-calib-characterize-y> endsub
