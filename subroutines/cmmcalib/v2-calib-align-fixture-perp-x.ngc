;v2-calib-align-fixture-perp-x
o<v2-calib-align-fixture-perp-x> sub
  #<yPos> = #1
  #<aPos> = #2
  #<bPos> = #3

  #<bAngle> = [#<bPos>]
  #<bAngleErr> = 0
  #<bErrAbs> = 0

  o100 do
    g0 b[#<bAngle>-5]
    g0 b[#<bAngle>]
    M66 E0 L0
    o<v2_calib_find_pos_fixture_rel_x_perp> call [#<yPos>] [#<aPos>] [#<bAngle>]
    #<bAngleErr> = [#<_value>]
    #<bAngle> = [#<bAngle> - #<bAngleErr>]
    #<bErrAbs> = [ ABS[#<bAngleErr>] ]
  o100 while [#<bErrAbs> GT 0.003]

o<v2-calib-align-fixture-perp-x> endsub [#<bAngle>]
M2