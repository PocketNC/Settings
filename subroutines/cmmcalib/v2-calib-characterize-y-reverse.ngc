;v2-calib-characterize-y-reverse
o<v2-calib-characterize-y-reverse> sub
  g21
  g90
  #<yMax> = -50.8
  #<yMin> = 50.8
  #<i> = 0
  #<numPoints> = #1
  g0 z0
  g0 x0
  g0 y-63.5a0b0

  o100 while [#<i> LT #<numPoints>]
    #<t> = [ #<i> / [ #<numPoints> - 1 ] ]
    #<y> = [[ #<yMax> * [ 1 - #<t> ]] + [ #<yMin> * #<t> ]]
    g0 y[#<y>]
    M66  E0 L0
    o<v2_calib_probe_y_reverse> call [#<y>]
    #<i> = [#<i> + 1]
  o100 endwhile

g0 Y-25.4
g0 Y0
M66  E0 L0
o<v2_calib_probe_y0_reverse> call [0]

o<v2-calib-characterize-y-reverse> endsub
M2
