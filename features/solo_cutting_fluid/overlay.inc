SCALE=3.33333
STEPGEN_MAX_VEL=2000
STEPGEN_MAX_ACC=10000

[PENTA]
ROCKHOPPER_CUTTING_FLUID_ON_SIG=cutting-fluid-on-1
ROCKHOPPER_CUTTING_FLUID_OFF_SIG=cutting-fluid-off-1
ROCKHOPPER_CUTTING_FLUID_TOGGLE_SIG=cutting-fluid-toggle-1
CUTTING_FLUID_TOGGLE_ON_SIG=cutting-fluid-on-2
CUTTING_FLUID_TOGGLE_OFF_SIG=cutting-fluid-off-2
